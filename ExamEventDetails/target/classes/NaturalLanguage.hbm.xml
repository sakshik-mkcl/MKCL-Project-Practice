<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	
	<hibernate-mapping package="com.app.model">
	<class name="NaturalLanguage" table="NATURALLANGUAGE">
	
	<id name="id" column="ID">
	<generator class="assigned" />
	</id>
	
	<property name="languageName" column="LANGUAGENAME" />
	<property name="nativeName" column="NATIVENAME" />
	<property name="isoCode" column="ISOCODE" />
	
	</class>
	</hibernate-mapping>